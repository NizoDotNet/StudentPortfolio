@page "{id}"
@model Portfolio.Pages.SubjectManager.SubjectModel
@{
}

<div class="container">
    <h5>@Model.SubjectVM.Name</h5>
    @foreach (var lab in Model.SubjectVM.LabWorks)
    {
        <p>@lab.Name</p>
    }
    <a asp-page="Update" asp-route-id="@Model.SubjectVM.Id" class="btn">Update</a>
    <form method="post" asp-page-handler="Delete"><button class="btn btn-danger">Delete</button></form>
</div>